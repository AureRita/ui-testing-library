{
    "compilerOptions": {
      "noImplicitAny": true,
      "noImplicitThis": true,
      "module": "es2022",
      "target": "es2022",
      "strict": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "allowJs": true,
      "downlevelIteration": true,
      "baseUrl": "./",
      "paths": {
        "@interfaces/*": ["src/interfaces/*"],
        "@pages/*": ["src/pages/*"],
        "@common/*": ["src/common/*"],
        "@data/*": ["src/data/*"],
        "@types/*": ["src/types/*"],
        "@utils/*": ["src/utils/*"],
        "@versions/*": ["src/versions/*"]
      },
      "typeRoots": ["types", "node_modules/@types"],
      "skipLibCheck": true,
      "noEmit": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }
  